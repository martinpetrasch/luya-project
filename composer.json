{
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "2.0.*@stable",
        "yiisoft/yii2-debug" : "2.0.*@stable",
        "twig/twig" : "1.*@stable",
        "bower-asset/jquery" : "2.1.*@stable",
        "bower-asset/jquery-ui" : "1.*@stable",
        "bower-asset/angular" : "1.3.10@stable",
        "bower-asset/angular-bootstrap" : "0.13.*@stable",
        "bower-asset/angular-i18n" : "1.3.10@stable",
        "bower-asset/angular-resource": "1.3.10@stable",
        "bower-asset/angular-ui-router" : "0.2.*@stable",
        "bower-asset/angular-loading-bar" : "0.*@stable",
        "bower-asset/angular-ui-ace" : "0.*@stable",
        "bower-asset/angular-dragdrop" : "1.*@stable",
        "bower-asset/ace-builds" : "1.1.8@stable",
        "bower-asset/twig.js" : "0.7.2@stable",
        "bower-asset/ng-flow" : "2.6.*@stable",
		"ext-curl" : "*",
        "curl/curl" : "1.2.0",
        "imagine/imagine": "~0.5.0",
        "phpmailer/phpmailer": "5.2.*@stable",
        "flowjs/flow-php-server" : "1.0.*@stable"
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload" : {
        "psr-4" : {
            "luya\\" : "../luya/src/",
            "admin\\" : "../luya/modules/admin",
            "cms\\" : "../luya/modules/cms",
            "cmsadmin\\" : "../luya/modules/cmsadmin",
            "newsadmin\\" : "../luya/modules/newsadmin",
            "news\\" : "../luya/modules/news",
            "account\\" : "../luya/modules/account",
            "errorapi\\" : "../luya/modules/errorapi",
            "gallery\\" : "../luya/modules/gallery",
            "galleryadmin\\" : "../luya/modules/galleryadmin",
            "calendar\\" : "../luya-modules/calendar",
            "calendaradmin\\" : "../luya-modules/calendaradmin"
        }
    }
}